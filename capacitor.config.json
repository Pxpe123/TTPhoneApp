{
  "appId": "tt.stats.app",
  "appName": "TTPhoneApp",
  "bundledWebRuntime": false,
  "webDir": "src",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "CapacitorHttp": {
      "enabled": true
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  },
  "server": {
    "androidScheme": "https",
    "cleartext": true
  }
}

